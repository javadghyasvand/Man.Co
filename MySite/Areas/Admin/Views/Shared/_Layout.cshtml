<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content='IE=edge' http-equiv=X-UA-Compatible>
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
    <meta name="description" content="@ViewBag.description">
    <meta name="author" content="@ViewBag.author">
    <title>@ViewBag.Title</title>
    <link rel="icon" href="~/images/ImageSite/logo.ico" type="image/gif" sizes="16x16">
    <!-- Core Css -->
    <link rel="stylesheet" type="text/css" href="~/Content/AdminAssets/assets/css/datatables.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/AdminAssets/assets/css/line-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/AdminAssets/assets/css/jquery.mCustomScrollbar.css">
    <link rel="stylesheet" type="text/css" href="~/Content/AdminAssets/assets/css/bootstrap-select.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/AdminAssets/assets/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/AdminAssets/assets/css/bootstrap-slider.css">
    <!-- Custom Css -->
    <link rel="stylesheet" type="text/css" href="~/Content/AdminAssets/assets/css/style.min.css">

</head>

<body style="direction: rtl">
    <div class="overlay-background"></div>
<div class="theme-setting-wrapper">
         <button type="button" id="settings-trigger" class="btn btn-primary waves-effect waves-primary"> 
             <i class="la la-cog"></i> 
         </button> 
         <div class="theme-setting-sidebar" style="direction: rtl;"> 
             <div class="h-100"> 
                 <div class="mt-4 d-flex align-items-start flex-wrap px-4"> 
                     <h4 class="font-weight-bold">تم پنل</h4> 
                 </div> 
                 <hr> 
                 <div class="theme-setting-sidebar-scroll"> 
                     <div class="px-4"> 
                         <div> 
                             <div class="d-flex align-items-center"> 
                                 <div class="radio theme-radio mr-4"> 
                                     <input type="radio" id="light" name="light" value="theme-light"> 
                                     <label for="light">روشن</label> 
                                 </div> 
                                 <div class="radio theme-radio mr-4"> 
                                     <input type="radio" id="dark" name="light" value="theme-dark"> 
                                     <label for="dark">تاریک</label> 
                                 </div> 
                             </div> 
                         </div> 
                     </div> 
                     <hr> 
                 </div> 
             </div> 
         </div> 
    </div>
<div class="br-sideleft">
        <a id="remove-menu" class="la la-close d-xl-none"></a>
        <div class="br-logo d-flex justify-content-center align-items-center">
            <a href="index.html">
                <img src="~/Content/AdminAssets/assets/image/logo.png" alt="MS Admin Panel" width="80" />
            </a>
        </div>
        <ul class="custom-scroll">
            <li class="active">
                <a href="index.html" class="waves-effect waves-primary">
                    <i class="la la-dashboard"></i>Dashboard
                </a>
            </li>
            <li>
                <a href="elements.html" class="waves-effect waves-primary"><i class="la la-clone"></i>Elements</a>
            </li>

            <li class="dropdown-sub-menu">
                <a href="javascript:;" class="waves-effect waves-primary">
                    <i class="la la-users"></i>Customers
                </a>
            </li>
            <li>
                <a href="javascript:;" class="waves-effect waves-primary">
                    <i class="la la-cart-arrow-down"></i>Orders
                </a>
            </li>
            <li>
                <a href="javascript:;" class="waves-effect waves-primary">
                    <i class="la la-wechat"></i>Messages
                </a>
            </li>
            <li>
                <a href="javascript:;" class="waves-effect waves-primary"><i class="la la-bank"></i>Payments</a>
            </li>
            <li>
                <a href="javascript:;" class="waves-effect waves-primary">
                    <i class="la la-image"></i>Media
                    Manager
                </a>
            </li>
            <li>
                <a href="javascript:;" class="waves-effect waves-primary"><i class="la la-cog"></i>Settings</a>
            </li>
        </ul>
    </div>
<header class="header fixed-top d-flex align-items-center">
      
        <div class="br-header d-flex w-100">
            <a id="add-menu" class="la la-navicon d-flex d-xl-none align-items-center justify-content-center"></a>
            <div class="br-header-left">
                <a href="javascript:;" class="searchbar-toggle la la-search d-flex d-md-none"></a>
                <form class="searchbar d-flex align-items-center pr-5">
                    <i class="la la-search"></i>
                    <input class="form-control border-0 pl-2" type="search" placeholder="Search...">
                </form>
            </div>
          
            <div class="br-header-right ml-auto">
                <nav class="nav">

                </nav>
            </div>
        </div>
</header>


    <!--START: MAIN PANEL-->
    <section class="mainpanel">
        <div class="pagebody">
            <div class="card mt-4 p-4">
                @RenderBody()
            </div>
     
        </div>
    </section>
    <!--END: MAIN PANEL-->
    <script src="~/Content/AdminAssets/assets/scripts/jquery.min.js"></script>
    <script src="~/Content/AdminAssets/assets/scripts/popper.min.js"></script>
    <script src="~/Content/AdminAssets/assets/scripts/bootstrap-slider.min.js"></script>
    <script src="~/Content/AdminAssets/assets/scripts/bootstrap.min.js"></script>
    <script src="~/Content/AdminAssets/assets/scripts/bootstrap.bundle.min.js"></script>
    <script src="~/Content/AdminAssets/assets/scripts/bootstrap-select.min.js"></script>
    <script src="~/Content/AdminAssets/assets/scripts/bootstrap-tooltip-custom-class.js"></script>
    <script src="~/Content/AdminAssets/assets/scripts/jquery.mCustomScrollbar.js"></script>
    <script src="~/Content/AdminAssets/assets/scripts/Chart.min.js"></script>
<script src="~/Content/AdminAssets/assets/scripts/datatables.min.js"></script>
    <script src="~/Content/AdminAssets/assets/scripts/ripple.min.js"></script>
<script src="~/Content/AdminAssets/assets/scripts/custome.js"></script>
<script src="~/Content/ckeditor/ckeditor.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    

</body>

</html>


